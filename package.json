{
  "name": "hapi-ip-whitelist",
  "version": "3.0.0",
  "description": "Hapi auth scheme that can be prepended to other auth schemes and used for filtering requests based on ip addresses",
  "repository": "https://github.com/FlorenceHC/hapi-ip-whitelist",
  "main": "lib/plugin.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "lab --globals code",
    "test-debug": "node --inspect-brk=0.0.0.0:9000 ./node_modules/.bin/lab -m 60000",
    "lint": "./node_modules/.bin/eslint ."
  },
  "dependencies": {
    "@hapi/boom": "^9.1.0"
  },
  "devDependencies": {
    "@hapi/code": "^8.0.1",
    "eslint": "^5.10.0",
    "@hapi/hapi": "^18.4.1",
    "@hapi/lab": "^22.0.4"
  },
  "peerDependencies": {
    "@hapi/hapi": "18.x"
  },
  "keywords": [
    "ip",
    "ip-whitelist",
    "auth",
    "scheme",
    "hapi",
    "plugin"
